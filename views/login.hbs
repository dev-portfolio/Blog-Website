<head>

    <link rel="stylesheet" href="assets/css/login_page.min.css" />
</head>

<body class="login_page">
<div class="login_page_wrapper">
    <div class="md-card" id="login_card">
        <div class="md-card-content large-padding" id="login_form">
            <div class="login_heading">
                <div class="user_avatar"></div>
            </div>
            <form method="post" action="/loggedin">

                <div class="uk-form-row">
                    <label for="login_email_reset"> Email Address</label>
                    <input class="md-input" type="email"  name="email" >
                </div>
                <div class="uk-form-row">
                    <label for="login_password">Password</label>
                    <input class="md-input" type="password"  name="password" >
                </div>

                <div class="uk-margin-medium-top">
                    <button class="md-btn md-btn-primary md-btn-block md-btn-large">Sign In</button>
                </div>
            </form>

            <div class="uk-margin-medium-top">

               <a href="/facebook1"> <button class="md-btn md-btn-primary md-btn-block md-btn-large">Login With Facebook</button> </a>
            </div>
            <br>
            <div class="uk-margin-top">
                <a href="/signup" id="login_help_show" class="uk-float-right">Creat New Account ?</a>

            </div>
        </div>
    </div>
</div>
</body>
