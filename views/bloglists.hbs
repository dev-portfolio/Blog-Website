

<div id="page_content">

    <div id="page_heading" data-uk-sticky="{ top: 48, media: 960 }">

        <h1>BLOG LISTs</h1>
        <span class="uk-text-upper uk-text-small"><a href="#">List of all blogs added by user</a></span>
    </div>

    <div id="page_content_inner">
        <div class="md-card">
            <div class="md-card-content">
                <div class="uk-overflow-container uk-margin-bottom">
                    <table class="uk-table uk-table-align-vertical uk-table-nowrap tablesorter tablesorter-altair" id="ts_issues">
                        <thead>
                        <tr>
                            <th class="uk-text-center">Key</th>
                            <th>Description</th>
                            <th>Date</th>

                        </tr>
                        </thead>
                        <tbody>
                       {{#each go}}
                        <tr>
                            <td class="uk-text-center"><span class="uk-text-small uk-text-muted uk-text-nowrap">{{this.id}}</span></td>

                            <td class="uk-text-md">{{this.description}}</td>
                            </td>
                            <td class="uk-text-small" >{{this.date}}</td>

                            <td class="uk-text-small" ><a href="/aditblog/{{this.id}}"><button class="md-btn md-btn-success" type="button" data-uk-button >Edit</button></a></td>
                            <td class="uk-text-small" ><a href="delete/{{this.id}}"><button class="md-btn md-btn-danger"  type="button" data-uk-button>Delete</button></a></td>
                        </tr>
{{/each}}
                        </tbody>
                    </table>
                </div>

                <br>


            </div>
        </div>
    </div>

</div>