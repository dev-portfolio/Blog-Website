<head><link rel="icon" type="image/png" href="assets/img/favicon-16x16.png" sizes="16x16">
<link rel="icon" type="image/png" href="assets/img/favicon-32x32.png" sizes="32x32">


</head>

<h3 class="heading_b uk-margin-bottom">BLOGS</h3>
{{#if blog1}}
    {{#each blog1}}
<div id="page_content">
    <div id="page_content_inner">


        <div class="blog_list uk-grid-width-medium-1-3 uk-grid-width-large-1-4" data-uk-grid="{gutter: 24}">
            <div>
                <div class="md-card">
                    <div class="md-card-content small-padding">
                        <img src="/uploads/{{this.photo}}" alt="" class="blog_list_teaser_image">
                        <div class="blog_list_teaser">
                            <h2 class="blog_list_teaser_title uk-text-truncate">{{this.title}}</h2>
                            <p>{{this.description}}</p>
                            <span class="uk-text-muted uk-text-small">{{this.date}}</span>
                        </div>
                        <div class="blog_list_footer">
                            <div class="blog_list_footer_info">
                                <span class="uk-margin-right"><i class="material-icons">&#xE87D;</i> <small>25</small></span>
                                <span><i class="material-icons">&#xE0B9;</i> <small>160</small></span>
                            </div>
                            <a href="page_blog_article.html" class="md-btn md-btn-small md-btn-flat md-btn-flat-primary uk-float-right">Read more</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
        {{/each}}
{{/if}}









